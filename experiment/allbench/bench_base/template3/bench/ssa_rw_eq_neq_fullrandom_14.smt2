(set-logic ALL)
(define-fun reg () RegLan (re.++ (re.range "m" "z") (re.* (re.range "m" "z"))))

(declare-const x String)
(declare-const y String)

(define-fun base () (Seq String) (seq.++ (seq.unit "a") (seq.unit "b") (seq.unit "c") (seq.unit "d") (seq.unit "e")))
(define-fun w_x_1 () (Seq String) (seq.write base 3 x))
(define-fun w_y_1 () (Seq String) (seq.write base 4 y))
(define-fun r_x_1 () String (seq.nth w_x_1 4))
(define-fun r_x_2 () String (seq.nth w_x_1 3))
(define-fun r_y_1 () String (seq.nth w_y_1 4))
(define-fun r_y_2 () String (seq.nth w_y_1 2))
(assert (str.in_re r_x_1 reg))
(assert (str.in_re r_y_1 (re.comp reg)))
(assert (str.in_re r_x_2 reg))
(assert (str.in_re r_y_2 (re.comp reg)))

(check-sat)
(get-model)