(set-logic QF_S)
(declare-const s (Seq String))
(define-fun v0 () (Seq String) (seq.unit "cat"))
(define-fun v1 () (Seq String) (seq.unit "dog"))
(define-fun v2 () (Seq String) (seq.++ v0 v1))
(define-fun v3 () (Seq String) (seq.write v2 1 "bird"))
(define-fun v4 () (Seq String) (seq.extract v3 0 2))
(define-fun v5 () String (seq.nth v4 1))
(define-fun v6 () Int (seq.len v4))
(assert (= v6 3))
(assert (= v5 "dog"))
(assert (not (str.in_re (seq.nth v4 0) (str.to_re "cat"))))
(check-sat)