(set-logic QF_S)
(declare-const s (Seq String))
(define-fun v0 () (Seq String) (seq.unit "a"))
(define-fun v1 () (Seq String) (seq.unit "b"))
(define-fun v2 () (Seq String) (seq.++ v0 v1))
(define-fun v3 () (Seq String) (seq.write v2 1 "xyz"))
(define-fun v4 () String (seq.nth v3 1))
(define-fun v5 () Int (seq.len v3))
(assert (str.in_re v4 (re.* (str.to_re "xyz"))))
(assert (= v5 2))
(check-sat)